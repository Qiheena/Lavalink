{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "numReplicas": 1,
    "sleepApplication": false,
    "restartPolicyType": "ON_FAILURE"
  },
  "services": [
    {
      "name": "lavalink",
      "serviceType": "web",
      "source": {
        "type": "image",
        "image": "built"
      },
      "domains": [
        {
          "name": "lavalink-production-70c6.up.railway.app"
        }
      ],
      "ports": [
        {
          "port": 2333,
          "protocol": "TCP",
          "public": true
        }
      ]
    }
  ]
}